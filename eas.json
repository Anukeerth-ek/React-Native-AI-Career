{
  "cli": {
    "version": ">= 3.13.0",
    "requireCommit": true,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_USE_NPM_INSTALL": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {}
    }
  },
  "submit": {
    "production": {}
  }
}
